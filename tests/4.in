int max(int x, int y) {
    if (x>y) return x;
    return y;
}

int b(int x) {
    return 2;
}

int a(int x, int y, int w) {
    int z;
    z=b(x);
    return (z+x)*max(y, w);
}

int gcd(int a, int b) {
    if (a == 0) return b;
    return gcd(b % a, a);
}

return gcd(12, 8) + gcd(21, 7) + gcd(30, 42) + a(4, 6, 8);
